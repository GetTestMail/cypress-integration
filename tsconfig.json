{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "lib": [
      "esnext"
    ]
  }
}